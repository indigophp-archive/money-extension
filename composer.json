{
    "name": "indigophp/money",
    "description": "Extensions for Money",
    "license": "MIT",
    "keywords": ["money", "extensions", "exchange"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "mathiasverraes/money": "dev-nextrelease"
    },
    "require-dev": {
        "indigophp/http-adapter": "dev-develop",
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
    },
    "suggest": {
        "indigophp/http-adapter": "Required for Exchange"
    },
    "autoload": {
        "psr-4": {
            "Money\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
